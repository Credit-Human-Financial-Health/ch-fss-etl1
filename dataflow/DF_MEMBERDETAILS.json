{
	"name": "DF_MEMBERDETAILS",
	"properties": {
		"folder": {
			"name": "FSS_DATAFLOW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "member_details_csv",
						"type": "DatasetReference"
					},
					"name": "MEMBERDETAILSSOURCE"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "member_details_sink",
						"type": "DatasetReference"
					},
					"name": "MEMBERDETAILSTABLE",
					"rejectedDataLinkedService": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "COLUMNSDERIVED"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {PWC_Key|ORDINAL|TYPE|SUBTYPE|MAILOVERRIDE|SSNOVERRIDE|SSNCERTIFICATION|SSNTYPE|EFFECTIVEDATE|EXPIRATIONDATE|BIRTHDATE|TITLE|FIRST|MIDDLE|LAST|SUFFIX|EXTRAADDRESS|STREET|CITY|STATE|ZIPCODE|HOMEPHONE|SSN|WORKPHONE|LASTFMDATE|CARRIERROUTE|ADDRESSTYPE|BENEFICIARYPERCENT|IRSCORRECTION|WORKPHONEEXTENSION|CTREXEMPT|DEATHDATE|ECOACODE|COUNTRY|COUNTRYCODE|EMAIL|MOTHERSMAIDENNAME|EMPLOYERNAME|ALTEMAIL|LOCATOR|MOBILEPHONE|PAGERNUMBER|USPERSONFLAG|IDENTDOCFLAG1|IDENTDOCFLAG2|IDENTDOCFLAG3|IDENTIDTYPE1|IDENTIDTYPE2|IDENTIDTYPE3|IDENTIDDESCRIPTION1|IDENTIDDESCRIPTION2|IDENTIDDESCRIPTION3|IDENTIDNUMBER1|IDENTIDNUMBER2|IDENTIDNUMBER3|IDENTIDISSUEDATE1|IDENTIDISSUEDATE2|IDENTIDISSUEDATE3|IDENTIDEXPIREDATE1|IDENTIDEXPIREDATE2|IDENTIDEXPIREDATE3|IDENTIDVERIFYDATE1|IDENTIDVERIFYDATE2|IDENTIDVERIFYDATE3|OCCUPATION|USERCHAR1|USERCHAR2|USERCHAR3|USERCHAR4|USERAMOUNT1|USERAMOUNT2|USERDATE1|USERDATE2|LASTADDRVERIFDATE|CREDITREPORTCONSUMERINFO|NAMEFORMAT|EXTENDEDNAME|CREDITREPORTCONSUMERINFODATE|EXTRAINFO|RECORDCHANGEDATE|MEMBERNUMBER|MBRSTATUS|MBRSTATUSFMDATE|MBRCREATEDATE|MBRFMLASTDATE|MBREXPDATE|MBRACTIVITYDATE|PHONETYPE|MBRADDRNUMBER|ADDRCREATEDATE|ADDRFMLASTDATE|ADDREXPDATE|NONACCTNAMENUM|MEMBERNUMLINK|MBRADDRNUMLINK|CURRGROSSMONTHPAY|CURRNETMONTHPAY|MBRFMLASTPURGEDATE|ADDRFMLASTPURGEDATE|FMLASTPURGEDATE|MEMBERNUMLINKCHGDATE|MBRADDRNUMLINKCHGDATE|MBRRECORDCHANGEDATE|MBRDISCONNECTDATE|ADDRRECORDCHANGEDATE|ADDRDISCONNECTDATE|MBRFILETYPE|MBRADDRFILETYPE|RESTRICTACCESS|CONFIDENTIAL|MARKETINGADDRESS|CREDITREPORTADDRINDICATOR|CREDITREPORTADDRINDICATORDATE|DBANAMEFORMAT|DBATITLE|DBAFIRST|DBAMIDDLE|DBALAST|DBASUFFIX|DBAEXTENDEDNAME|SEX|IRSCOUNTRYCODE|NRATAXRATE|NRAEXEMPTIONCODE|FORMW8ONFILE|W8EXPIRATIONDATE|FOREIGNTIN|GIIN|SUBSTANTIALOWNER|CHAPTER4STATUSCODE|PREFERREDCONTACTMETHOD|ACTIVEDUTY|ACTIVEDUTYVERIFICATIONDATE|ACTIVEDUTYSTARTDATE|ACTIVEDUTYSEPARATIONDATE|LOB|LEI|BENEFICIALOWNER|BENEFICIALPERCENT|IRS1042SAMENDNUM|HEADQUARTERS|MBRVERSIONID|MBRUUID|MBRADDRVERSIONID|MBRADDRUUID|LASTADDRCHGDATE|CRNAMECHGDATE|CRADDRCHGDATE|CRSSNCHGDATE|CURRMTHPAYCHGDATE} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> MEMBERDETAILSSOURCE",
				"MEMBERDETAILSSOURCE derive(name = replace(FirstName, char(0), ''),",
				"          {e-mail} = LastName,",
				"          state = substring(rtrim(STATE), 1, 2)) ~> COLUMNSDERIVED",
				"COLUMNSDERIVED sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          member_id as integer,",
				"          name as string,",
				"          last_name as string,",
				"          date_of_birth as date,",
				"          ssn as string,",
				"          address as string,",
				"          city as string,",
				"          state as string,",
				"          zip_code as string,",
				"          phone as string,",
				"          email as string,",
				"          branch as string,",
				"          status as string,",
				"          start_date as date,",
				"          created_date as timestamp,",
				"          created_by as string,",
				"          updated_date as timestamp,",
				"          updated_by as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          member_id = PWC_Key,",
				"          name,",
				"          last_name = LastName,",
				"          address = STREET,",
				"          city = CITY,",
				"          state,",
				"          zip_code = ZIPCODE,",
				"          start_date = MBRCREATEDATE,",
				"          email = {e-mail}",
				"     )) ~> MEMBERDETAILSTABLE"
			]
		}
	}
}