{
	"name": "member_details",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "member_detailscsv",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "member_details_csv",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          add_date as string,",
				"          PWC_Key as integer,",
				"          ORDINAL as string,",
				"          TYPE as string,",
				"          SUBTYPE as string,",
				"          MAILOVERRIDE as string,",
				"          SSNOVERRIDE as string,",
				"          SSNCERTIFICATION as string,",
				"          SSNTYPE as string,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as short,",
				"          DEATHDATE as string,",
				"          ECOACODE as string,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as string,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source",
				"source derive(PWC_Key = ERROR_FUNCTION('')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          add_date as string,",
				"          PWC_Key as string,",
				"          ORDINAL as string,",
				"          TYPE as string,",
				"          SUBTYPE as string,",
				"          MAILOVERRIDE as string,",
				"          SSNOVERRIDE as string,",
				"          SSNCERTIFICATION as string,",
				"          SSNTYPE as string,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as string,",
				"          DEATHDATE as string,",
				"          ECOACODE as string,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as string,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as string",
				"     ),",
				"     partitionFileNames:['member_details_csv'],",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}