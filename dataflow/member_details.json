{
	"name": "member_details",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "member_detailscsv",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "member_details_csv",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          add_date as string,",
				"          PWC_Key as integer,",
				"          ORDINAL as boolean,",
				"          TYPE as short,",
				"          SUBTYPE as boolean,",
				"          MAILOVERRIDE as boolean,",
				"          SSNOVERRIDE as boolean,",
				"          SSNCERTIFICATION as boolean,",
				"          SSNTYPE as boolean,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as short,",
				"          DEATHDATE as string,",
				"          ECOACODE as boolean,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as string,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source",
				"source sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PWC_Key as string,",
				"          Shr as string,",
				"          {Beg Balance} as string,",
				"          {#Dep} as string,",
				"          {Dep Amount} as string,",
				"          {#With} as string,",
				"          {With Amount} as string,",
				"          {# Dep External Transfers} as string,",
				"          {Dep External Transfers Bal} as string,",
				"          {# With External Transfers} as string,",
				"          {With External Transfers Bal} as string,",
				"          {# Dep Internal Transfers} as string,",
				"          {Dep Internal Transfers Bal} as string,",
				"          {# With Internal Transfers} as string,",
				"          {With Internal Transfers Bal} as string,",
				"          {# NSF CP Fees} as string,",
				"          {NSF CP Fees Amt} as string,",
				"          {End Balance} as string,",
				"          {Monthly Avg Balance} as string,",
				"          SType as string,",
				"          Description as string,",
				"          City as string,",
				"          State as string,",
				"          Zip as string,",
				"          YMbr as string,",
				"          Age as string,",
				"          {Orig Branch} as string,",
				"          {Cur Branch} as string,",
				"          {Aggr Daily End Bal} as string,",
				"          {# of Days Open During Month} as string",
				"     ),",
				"     partitionFileNames:['member_details_csv'],",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}