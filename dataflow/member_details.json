{
	"name": "member_details",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "member_details_csv",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "member_details_sink",
						"type": "DatasetReference"
					},
					"name": "sink",
					"rejectedDataLinkedService": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          add_date as string,",
				"          PWC_Key as integer,",
				"          ORDINAL as string,",
				"          TYPE as string,",
				"          SUBTYPE as string,",
				"          MAILOVERRIDE as string,",
				"          SSNOVERRIDE as string,",
				"          SSNCERTIFICATION as string,",
				"          SSNTYPE as string,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as string,",
				"          DEATHDATE as string,",
				"          ECOACODE as string,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as string,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source",
				"source derive(name = replace(FirstName, char(0), ''),",
				"          {e-mail} = LastName,",
				"          state = substring(rtrim(STATE), 1, 2)) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          member_id as integer,",
				"          name as string,",
				"          last_name as string,",
				"          date_of_birth as date,",
				"          ssn as string,",
				"          address as string,",
				"          city as string,",
				"          state as string,",
				"          zip_code as string,",
				"          phone as string,",
				"          email as string,",
				"          branch as string,",
				"          status as string,",
				"          start_date as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          member_id = PWC_Key,",
				"          name,",
				"          last_name = LastName,",
				"          address = STREET,",
				"          city = CITY,",
				"          state = STATE,",
				"          zip_code = ZIPCODE,",
				"          start_date = MBRCREATEDATE,",
				"          email = {e-mail}",
				"     )) ~> sink"
			]
		}
	}
}