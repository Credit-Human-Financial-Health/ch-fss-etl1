{
	"name": "member_details",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "member_detailscsv",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "member_details_csv",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          add_date as string,",
				"          PWC_Key as short,",
				"          ORDINAL as boolean,",
				"          TYPE as short,",
				"          SUBTYPE as boolean,",
				"          MAILOVERRIDE as boolean,",
				"          SSNOVERRIDE as boolean,",
				"          SSNCERTIFICATION as boolean,",
				"          SSNTYPE as boolean,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as short,",
				"          DEATHDATE as string,",
				"          ECOACODE as boolean,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as short,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source",
				"source derive(name = replace(FirstName, '\\u0000', ''),",
				"          {last_name     } = replace(LastName, '\\u0000', '')\r",
				",",
				"          address = replace(STREET, '\\u0000', ''),",
				"          city = replace(CITY, '\\u0000', ''),",
				"          state = replace(STATE, '\\u0000', '')\r",
				",",
				"          zip_code = replace(ZIPCODE, '\\u0000', '')\r",
				",",
				"          status = toString(MBRSTATUS),",
				"          start_date = toDate(trim(replace(MBRCREATEDATE, ' +00', '')), 'yyyy-MM-dd'),",
				"          member_id = toInteger(PWC_Key, '[^0-9]', '')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {PWC_Key|Shr|Beg Balance|#Dep|Dep Amount|#With|With Amount|# Dep External Transfers|Dep External Transfers Bal|# With External Transfers|With External Transfers Bal|# Dep Internal Transfers|Dep Internal Transfers Bal|# With Internal Transfers|With Internal Transfers Bal|# NSF CP Fees|NSF CP Fees Amt|End Balance|Monthly Avg Balance|SType|Description|City|State|Zip|YMbr|Age|Orig Branch|Cur Branch|Aggr Daily End Bal|# of Days Open During Month} as string",
				"     ),",
				"     partitionFileNames:['member_details_csv'],",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}