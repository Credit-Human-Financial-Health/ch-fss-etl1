{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "credithumandf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzurePostgreSql1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzurePostgreSql1'"
		},
		"PostgreSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PostgreSql'"
		},
		"financial_slack_score_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'financial_slack_score'"
		},
		"AzurePostgreSql1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "credithumandb.postgres.database.azure.com"
		},
		"AzurePostgreSql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "credit_human_db"
		},
		"AzurePostgreSql1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "credithumanadmin"
		},
		"PostgreSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "credithumandb.postgres.database.azure.com"
		},
		"PostgreSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "credit_human_db"
		},
		"PostgreSql_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "credithumanadmin"
		},
		"financial_slack_score_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "credithumandb.postgres.database.azure.com"
		},
		"financial_slack_score_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "credit_human_db"
		},
		"financial_slack_score_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "credithumanadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "credithumandf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzurePostgreSql1_password": {
						"value": "[parameters('AzurePostgreSql1_password')]"
					},
					"PostgreSql_password": {
						"value": "[parameters('PostgreSql_password')]"
					},
					"financial_slack_score_password": {
						"value": "[parameters('financial_slack_score_password')]"
					},
					"AzurePostgreSql1_properties_typeProperties_server": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_server')]"
					},
					"AzurePostgreSql1_properties_typeProperties_database": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_database')]"
					},
					"AzurePostgreSql1_properties_typeProperties_username": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_username')]"
					},
					"PostgreSql_properties_typeProperties_server": {
						"value": "[parameters('PostgreSql_properties_typeProperties_server')]"
					},
					"PostgreSql_properties_typeProperties_database": {
						"value": "[parameters('PostgreSql_properties_typeProperties_database')]"
					},
					"PostgreSql_properties_typeProperties_username": {
						"value": "[parameters('PostgreSql_properties_typeProperties_username')]"
					},
					"financial_slack_score_properties_typeProperties_server": {
						"value": "[parameters('financial_slack_score_properties_typeProperties_server')]"
					},
					"financial_slack_score_properties_typeProperties_database": {
						"value": "[parameters('financial_slack_score_properties_typeProperties_database')]"
					},
					"financial_slack_score_properties_typeProperties_username": {
						"value": "[parameters('financial_slack_score_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "credithumandf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'credithumandf_ArmTemplate_0')]"
			]
		},
		{
			"name": "credithumandf_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'credithumandf_ArmTemplate_1')]"
			]
		}
	]
}